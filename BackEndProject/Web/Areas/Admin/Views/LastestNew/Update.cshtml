@using Core.Constants
@using Web.Areas.Admin.ViewModels.LastestNew
@model LastestNewUpdateVM

<form enctype="multipart/form-data" asp-action="update" asp-controller="lastestnew" method="post">

     <div class="form-group">
        <label asp-for="Title">Title</label>
        <input class="form-control" asp-for="Title">
        <span asp-validation-for="Title"></span>
    </div>
    <div class="form-group">
        <label asp-for="Text">Text</label>
        <input class="form-control" asp-for="Text">
        <span asp-validation-for="Text"></span>
    </div>
    <div class="form-group">
        <label asp-for="Time">Time</label>
        <input class="form-control" asp-for="Time">
        <span asp-validation-for="Time"></span>
    </div>
    <div class="form-group">
        <label asp-for="MainPhoto">MainPhoto</label>
        <input class="form-control" asp-for="MainPhoto">
        <span asp-validation-for="MainPhoto"></span>
    </div>


    <button type="submit" class="btn btn-primary">Uptade</button>
   <a class="btn btn-danger" asp-action="index" asp-controller="lastestnew">Cancel</a>
</form>
